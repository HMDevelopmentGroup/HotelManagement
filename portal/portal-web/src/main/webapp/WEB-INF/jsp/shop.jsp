<script type="text/javascript">
    var isLogin = "0";

    $(function () {

        /*签到有好礼弹窗*/
        $("#regist").click(function () {
            if (isLogin == "1") {
                getSignInContent();
                $(".mask").show(500);
                $(".regist_gotoPopup").show(700);
            }
            else { alert("请先登录"); }
        })
        $(".rcm_list li").hover(function () {
            $(this).children("div").addClass("hver");
        }, function () {
            $(this).children("div").removeClass("hver");
        })
        $(document).on("click", ".reg_del", function () {
            $(".mask").hide(500);
            $(".regist_gotoPopup").hide(500);
        })

        /*fixed*/
        var win = $(window);
        var sc = $(document);

        var psc = $(".product .title").position().top - 10;
        win.scroll(function () {
            if (sc.scrollTop() >= 140) {
                $(".nav,.bread").addClass("fixed");

            } else {
                $(".nav,.bread").removeClass("fixed");
            }
        })
        $(".block .img img").lazyload({
            placeholder: "/content/images/loading.gif",
            effect: "fadeIn"
        });

        $(".block .img img").lazyload({
            placeholder: "/content/images/loading.gif",
            effect: "fadeIn"
        });

        $("#home-back-to-top").css("visibility", "hidden");
        win.scroll(function () {
            if (win.scrollTop() > 100) {
                $("#home-back-to-top").css("visibility", "visible");
            }
            else {
                $("#home-back-to-top").css("visibility", "hidden");
            }
        });
        $("#home-back-to-top").click(function () {
            $('body,html').animate({ scrollTop: 0 }, 400);
            return false;
        });

        /*img*/
        var anima;
        $(".product .img img").mouseover(function () {
            ani(this);
        }).mouseout(function () {
            clearTimeout(anima);
            $(this).animate({ opacity: 1 }, 100);
            $(this).siblings().animate({ opacity: 1 }, 100);
        });

        function ani(obj) {
            if ($(obj).prev().length < 1) {
                $(obj).animate({ opacity: 1 }, 100);
                $(obj).siblings().css("z-index", "10").animate({ opacity: 1 }, 100);
                anima = setTimeout(function () {
                    ani($(obj).siblings().last());
                }, 1000);
                return;
            }
            $(obj).animate({ opacity: 0 }, 100);
            var prev = $(obj).prev();
            anima = setTimeout(function () {
                ani(prev);
            }, 1000);
        }
        /*flyer*/
        var offset = $(".disp").offset();
        $(".product .addcart").click(function (event) {
            if ($(this).attr("href") != "javascript:;") {
                return true;
            }
            var productid = $(this).parent().find(".ProductId").val();
            var priceId = $(this).parent().find(".DefaultPriceID").val();//$('a[name="price"].active');
            if (priceId == null || priceId == undefined) {
                return;
            }
            var amount = 1;
            if (isNaN(parseInt(priceId))) {
                alert("价格不正确");
                return false;
            }
            var ProductCurrentDepot = $(this).parent().find(".ProductCurrentDepot").val();
            if (ProductCurrentDepot == undefined) {
                alert("库存不足");
                return false;
            }
            if (parseInt(ProductCurrentDepot) < parseInt(amount)) {
                alert("当前库存不足");
                return false;
            }
            var obj = $(this);
            addCart(productid, amount, priceId, function () { addCartResult(offset, event, obj); });
            return false;
        });

        var searchArray = location.search.split('&');//获取URL的search部分并且以"&"为界限分割
        for (var i = 0; i < searchArray.length; i++) {
            var values = searchArray[i].split('=');
            if (values.length > 1 && values[0].replace('?', '') == 'flag' && values[1] == 'sign') {
                $("#regist").click();
            }
        }

    });

    function deleteClass(obj) {
        $('.active[data-role="class"]').removeClass('active');
        $('#li_class').show();
        $(obj).parent().remove();
        $('#chosen').children().remove();
    }

    function addCartResult(offset, event, obj) {
        getShoppingCartCount();//更新购物车数量
        var addcar = obj; $(this);//$(".product_details .btn_red");
        var img = addcar.parents(".block").find('img').last().attr('src');
        var flyer = $('<img class="flyer" src="' + img + '">');
        flyer.fly({
            start: {
                left: event.pageX,
                top: event.pageY
            },
            end: {
                left: offset.left + 10,
                top: offset.top + 10,
                width: 0,
                height: 0
            },
            onEnd: function () {
                $("#cart_success").show().animate({ display: 'block' }, 200).fadeOut(1000);
                this.destory();
            }
        });
    }

    function getSignIn() {
        $.ajax({
            url: '/tool/SignIn',// 跳转到 action
            data: {
                r: (new Date()).getMilliseconds()
            },
            type: 'post',
            dataType: 'json',
            cache: false,
            success: function (data) {
                if (!data.Success) {
                    alert(data.Message);
                    return false;
                }
                else {
                    getSignInContent();
                    alert("签到成功");
                }
            }
        });
    }
    //图片轮播
    var ie6 = !-[1, ] && !window.XMLHttpRequest;
    var userAgent = navigator.userAgent.toLowerCase();
    var browser = {
        ie8: /msie 8/.test(userAgent),
        ie7: /msie 7/.test(userAgent)
    };
    var len;
    var kuan;
    var banner_w;
    function picResize() {
        len = $(".banner li").length;
        kuan = $(window).width();
        banner_w = kuan * len;
        $(".banner ul").css('width', banner_w);
        $(".banner li").css('width', kuan);
        $(".banner li img").css('width', kuan);

    }
    $(document).ready(function () {

        len = $(".index_pic li").length;
        var i = 1;
        var lunhuan;
        kuan = $(window).width();
        var height = $(".index_pic li").find("img").height();
        banner_w = kuan * len;
        var h_w = $("#hao").width();
        $("#hao").css("margin-left", -h_w / 2 + "px");
        $("#prev,#next").css("top", height / 2 - 42 + "px");
        $(".banner,.index_pic li").height(height);
        $(".index_pic ul").css('width', banner_w);
        $(".index_pic li").css('width', kuan);
        $(".xu:eq(0)").css('background-position', '0 -25px');


        function change() {
            $(".xu:eq(" + i + ")").css('background-position', '0 -25px');
            $(".xu:eq(" + i + ")").siblings().css('background-position', '0 0');
        }

        function huan() {
            change();
            //$(".banner li:eq("+i+")").siblings("li").fadeOut(500);
            //$(".banner li:eq("+i+")").fadeIn(1500);
            if (i < 0)
            { i = 0 }
            if (i < len) {
                $(".banner ul").animate({ left: -kuan * i });
                i = i + 1;
            }
            else {
                i = len;
            }
            if (i == len)
            { i = 0 }


        }
        lunhuan = setInterval(huan, 4000);
        $("#hao").find(".xu").hover(function () {
            j = $(this).index();
            $(".xu:eq(" + j + ")").css('background-position', '0 -25px');
            $(".xu:eq(" + j + ")").siblings().css('background-position', '0 0');
            i = j;
            //$(".banner li:eq("+j+")").siblings("li").fadeOut(500);
            //$(".banner li:eq("+j+")").fadeIn(2000);
            $(".banner ul").animate({ left: -kuan * j });

        })
        $("#prev").click(function () {
            //clearInterval(lunhuan);
            //alert(i);
            if (i == len) { --i }
            if (i >= 0) {
                change();
                $(".banner ul").animate({ left: -kuan * i });
                i--;
            }
            else {
                i = 0;
            }


        })
        $("#next").click(function () {
            //clearInterval(lunhuan);
            if (i == 0) { ++i }
            if (i < len) {
                change();
                $(".banner ul").animate({ left: -kuan * i });
                i++;
            }
            else {
                i = len;
            }
        })

    })

    $(function () {
        var win = $(window); //得到窗口对象
        var sc = $(document);//得到document文档对象
        $(".mask").height(sc.height());
    })
    //左侧飘浮菜单
    $(function () {
        var win = $(window); //得到窗口对象
        var sc = $(document);//得到document文档对象
        $(".sldBar_menu").css("display", "none");
        win.scroll(function () {
            if (sc.scrollTop() >= 300) {
                $(".sldBar_menu").slideDown(300);
            } else {
                $(".sldBar_menu").slideUp(400);
            }
        })
        $(".sldBar_menu li.disp").find("a").show();
        $(".sldBar_menu li").find(".sld_txt").hide();
        $(".sldBar_menu li").hover(
            function () {
                $(this).find(".sld_txt").show();
                $(this).find("i").addClass("i_redIcon");
                $(".sldBar_menu li.disp").find("a").show();
            },
            function () {
                $(this).find(".sld_txt").hide()
                $(this).find("i").removeClass("i_redIcon");
                $(".sldBar_menu li.disp").find("a").show();
            })
        $("#back_top").click(function () {
            $('body,html').animate({ scrollTop: 0 }, 400);
            return false;
        });
    })
</script>



